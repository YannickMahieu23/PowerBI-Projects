	column top_bottom_missions = ```
			
			Var bottom =
			RANKX('space missions top5bottom5'
			    ,'space missions top5bottom5'[missions] -- use index to sort the rank on to prevent ties
			    , 
			    , ASC
			    , dense
			)
			Var topp =
			RANKX('space missions top5bottom5'
			    ,'space missions top5bottom5'[missions]
			    , 
			    , DESC
			    , dense
			)
			
			RETURN
			bottom >= 1 && bottom <= 3
			||
			topp >= 1 && topp <= 3
